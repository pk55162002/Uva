#include<iostream>
#include<math.h>
using namespace std;
int main(){
    int a, b, c, d, e, i, j, f1, f2;
    while(cin>>a){
        for(i=1;i<=a;i++){
            cin>>b;
            f1 = 1;
            c = b;
            while(c>1){
                if(c%2 == 1)f1++;
                c/=2;
            }
            f2 = 0;
            while(b%10!=0){
                e = b%10;
                if(e == 0)continue;
                else{
                    if(e%2 == 0 || e%2 == 1)f2++;
                    while(e>1){
                        if(e%2 == 1)f2++;
                        e/=2;
                    }
                } 
                b/=10;    
            }
            cout<<f1<<" "<<f2<<endl;
        }
    }
} 
